{
    "name": "lexik/monolog-browser-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony2 bundle provides a Doctrine DBAL handler for Monolog and a web UI to display log entries",
    "keywords": ["Symfony2", "bundle", "monolog", "logger", "doctrine"],
    "homepage": "https://github.com/lexik/LexikMonologBrowserBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Barthe",
            "email": "j.barthe@lexik.fr"
        },
        {
            "name": "Dev Lexik",
            "email": "dev@lexik.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~2.7|~3.0",
        "knplabs/knp-paginator-bundle": ">=2.3"
    },
    "require-dev": {
        "monolog/monolog": "~1.2",
        "doctrine/dbal": "~2.4"
    },
    "autoload": {
        "psr-4": { "Lexik\\Bundle\\MonologBrowserBundle\\": "" }
    }
}
